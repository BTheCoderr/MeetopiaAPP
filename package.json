{
  "name": "meetopia-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "build": "prisma generate && cd server && tsc && cd .. && next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node prisma/seed.mjs",
    "postinstall": "prisma generate",
    "signal": "cd server && node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/socket.io": "^3.0.1",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "emoji-picker-react": "^4.6.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "postcss": "^8.4.49",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2"
  },
